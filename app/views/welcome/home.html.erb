<%# Add the Facebook login link here %>
<%#= link_to 'Log in with Facebook!', '/auth/facebook' %>
<%# add conditional logic to the view to chaeck to see if the user is signed in or not%>
<% if session[:user_id] %>
  <h1><%= @user.name %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>Facebook UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
<% else %>
  <%= link_to 'Log in with Facebook!', 'auth/facebook' %>
<% end %>